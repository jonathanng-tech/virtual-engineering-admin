{
  "name": "virtual-engineering-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode=dev",
    "build:dev": "tsc -b || true && vite build --mode=dev",
    "build:staging": "tsc -b || true && vite build --mode=staging",
    "build:prod": "tsc -b || true && vite build --mode=prod",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format-check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{js,jsx,mjs,ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{js,jsx,mjs,ts,tsx,mdx}\" --cache",
    "lint": "yarn eslint && yarn prettier"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.1.0",
    "@mui/material": "^6.1.0",
    "@react-three/cannon": "^6.6.0",
    "@react-three/drei": "^9.112.0",
    "@react-three/fiber": "^8.17.7",
    "@tweenjs/tween.js": "^25.0.0",
    "@types/lodash": "^4.17.7",
    "@types/three": "^0.168.0",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "eventemitter3": "^5.0.1",
    "formik": "^2.4.6",
    "framer-motion": "^11.5.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-google-recaptcha-v3": "^1.10.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.53.0",
    "react-joyride": "^2.9.2",
    "react-router-dom": "^6.26.2",
    "react-toastify": "^10.0.5",
    "styled-components": "^6.1.13",
    "three": "^0.168.0",
    "three-bvh-csg": "^0.0.16",
    "yup": "^1.4.0",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1"
  }
}
